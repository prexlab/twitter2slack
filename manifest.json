{
	"name": "twitter2slack",
	"manifest_version": 2,
	"version": "1.12",
	"description": "twitter2slack",
	"icons": {
	  "128": "slack.png"
	}
	,"background": {
		"scripts" : ["background.js"]
	}
	,"browser_action": {
		  "default_icon": "slack.png"
	}
	,"permissions": [
		  "notifications"
		, "tabs"
		, "http://*/*"
		, "https://*/*"
	]
}
